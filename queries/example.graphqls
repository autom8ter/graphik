mutation createNode {
    createNode(input: {
        _type: "user"
        name: "coleman"
    }) {
        attributes
    }
}

query {
    nodes(input:{type:"user" limit: 1}) {
        attributes
    }
}

mutation delNode {
    delNode(input: {
        _id: "6e96b7c8-de83-1ca8-4673-8897ffce8602",
        _type: "user"
    }) {
        count
    }
}